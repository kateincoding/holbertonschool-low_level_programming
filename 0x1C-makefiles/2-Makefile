NAME		=	holberton
#***************** SRC ********************#

SRC			=	main.c holberton.c 

#***************** DEPS *******************#

CC			=	gcc

#************************ DEPS COMPILATION *************************

OBJ = $(SRC:%.c=%.o)

#************************ MAIN COMPILATION *************************

all		:		$(OBJ)
				$(CC) $(OBJ) -o $(NAME)
